<launch>
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen" clear_params="true">
    <rosparam file="$(find libro_model)/config/local_costmap_params.yaml" command="load" ns="local_costmap"/>
    <rosparam file="$(find libro_model)/config/global_costmap_params.yaml" command="load" ns="global_costmap"/>
    <rosparam file="$(find libro_model)/config/local_planner_params.yaml" command="load" ns="DWAPlannerROS"/>
    <rosparam file="$(find libro_model)/config/move_base_params.yaml" command="load" />
    <rosparam file="$(find libro_model)/config/global_planner_params.yaml" command="load" ns="GlobalPlanner"/>
  </node>
</launch>